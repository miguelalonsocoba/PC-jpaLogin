spring.application.name=jpaLogin
spring.output.ansi.enabled=always

application.title = Login
application.description = Prueba de concepto para el manejo de Autenticacion y Autorizacion
application.version = 1.0.0

spring.datasource.driver-class-name=com.postgresql.Driver
spring.datasource.url=jdbc:postgresql://localhost:5432/basicLoginDB
spring.datasource.username=postgres
spring.datasource.password=admin

spring.jpa.open-in-view=false
spring.jpa.generate-ddl=false
spring.jpa.hibernate.ddl-auto=none
spring.jpa.show-sql=true
